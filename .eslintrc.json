{
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": 0,
        "strict": [
            "error",
            "global"
        ],
        "no-const-assign": "error",
        "no-use-before-define": [
            "error",
            {
                "functions": false
            }
        ],
        "no-undef": "error",
        "require-jsdoc": "off",
        "max-len": [
            "error",
            {
                "ignoreTemplateLiterals": true,
                "ignoreStrings": true
            }
        ],
        "no-invalid-this": "off",
        "no-unused-vars": [
            "error",
            {
                "vars": "local"
            }
        ]
    }
}